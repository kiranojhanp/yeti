{
  "name": "yeti-vscode",
  "displayName": "Yeti",
  "version": "0.0.1-beta.2",
  "description": "A lightweight and expressive database markup language",
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kiranojhanp/yeti.git"
  },
  "scripts": {
    "package": "vsce package"
  },
  "contributes": {
    "grammars": [
      {
        "language": "yeti",
        "scopeName": "source.yeti",
        "path": "./syntaxes/yeti.tmLanguage.json"
      }
    ],
    "languages": [
      {
        "id": "yeti",
        "aliases": [
          "Yeti",
          "yeti"
        ],
        "extensions": [
          ".yeti"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./images/logo.webp",
          "dark": "./images/logo.webp"
        }
      }
    ]
  },
  "engines": {
    "vscode": "^1.96.0"
  },
  "icon": "images/logo.webp",
  "vsce": {
    "dependencies": false
  }
}
